<h1> Edit Itinerary </h1>
<br>
<br>
<%= form_for (@itinerary) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
    
  <%= f.label :location %>
  <%= f.text_field :location %>
     
  <%= f.label :description %>
  <%= f.text_field :description %>
    
  <%= f.submit "Update Destination" %>
  
<% end %>

